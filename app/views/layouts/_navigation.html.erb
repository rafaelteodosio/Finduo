<% if current_user%>

  <li><%= link_to "Home", root_path %></li>
  <li><%= link_to "Ver Perfil", current_user %></li>
  <li><%= link_to "Editar Perfil", edit_user_registration_path %></li>
  <li><%= link_to "Sair", destroy_user_session_path, :method => "delete" %></li>
<% elsif current_admin%>
  <li><%= link_to "Sair", destroy_admin_session_path, :method => "delete" %></li>
<% else %>
  <li><%= link_to "Entrar", new_user_session_path %></li>
  <li><%= link_to "Registrar", new_user_registration_path %></li>
  
<% end %>
